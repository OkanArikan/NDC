{
  "files.exclude": {
    "**/__pycache__": true,
    "**/__init__.py": true,
    "**/node_modules": true,
    ".dev/Dependencies": true
  },
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/.hg/store/**": true,
    ".dev": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    ".dev": true,
    "submodules/**": true
  },
  "cmake.configureOnOpen": true,
  "cmake.cmakePath": "${workspaceFolder}/../Dev/.dev/brew/bin/cmake",
  "cmake.buildDirectory": "${workspaceFolder}/.dev/intermediates/vscode",
  "cmake.configureEnvironment": {
    "BUILD_PRODUCTS_HOME": "${workspaceFolder}/.dev/products/vscode"
  },
  "telemetry.enableTelemetry": false,
  "editor.formatOnSave": true,
  "editor.detectIndentation": false,
  "editor.tabSize": 4,
  "C_Cpp.clang_format_path": "${workspaceFolder}/../Dev/.dev/brew/bin/clang-format",
  "C_Cpp.default.includePath": [
    "${default}",
    "${workspaceFolder}",
    "${workspaceFolder}/submodules",
    "${workspaceFolder}/../BuiltDependencies/install/common/include",
    "${workspaceFolder}/../BuiltDependencies/install/platforms/macosx/include",
    "${workspaceFolder}/../Dev/.dev/brew/include"
  ],
  //"C_Cpp.clang_format_path": "${workspaceFolder}/../Dev/.dev/brew/bin/clang-format",
  "C_Cpp.default.cppStandard": "c++20",
  "python.globalModuleInstallation": false,
  "python.formatting.provider": "autopep8",
  "python.formatting.autopep8Args": ["--max-line-length=2000"],
  "python.linting.enabled": true,
  "python.linting.flake8Enabled": true,
  "python.linting.flake8Args": ["--ignore", "E501"],
  "python.linting.pylintEnabled": false,
  "python.testing.unittestArgs": ["-v", "-s", ".", "-p", "*test.py"],
  "python.testing.pytestEnabled": false,
  "python.testing.nosetestsEnabled": false,
  "python.testing.unittestEnabled": true,
  "terminal.integrated.shell.osx": "/bin/zsh",
"files.associations": {
    "ios": "cpp",
    "random": "cpp",
    "algorithm": "cpp",
    "geometry": "cpp",
    "core": "cpp"
},
"C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
}
